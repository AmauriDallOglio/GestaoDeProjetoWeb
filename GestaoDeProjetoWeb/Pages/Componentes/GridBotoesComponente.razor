
<div class="adicionaEspaco adicionarBorda tabela-Largura adicionarCorInterna">
    <div class="col-md-auto">
        <button type="button" class="botao-Customizado botao-Customizado-inserir" @onclick="Inserir">

            <ion-icon class="botao-Customizado-Icone" name="add-circle-outline"></ion-icon> Inserir
        </button>
        <button type="button" class="botao-Customizado botao-Customizado-excluir" @onclick="Excluir" disabled>
            <ion-icon class="botao-Customizado-Icone" name="trash-outline"></ion-icon>
            Excluir
        </button>
 
    </div>
</div>

@code {
    [Parameter] public EventCallback Inserir { get; set; }
    [Parameter] public EventCallback Excluir { get; set; }
    [Parameter] public bool BotaoExcluirHabilitado { get; set; }
}
